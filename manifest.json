{
    "manifest_version": 3,
    "name": "Tab Muter",
    "version": "0.0.1",
    "description": "A simple muting Chrome Extension that mutes all mutable tabs.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}