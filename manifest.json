{
    "manifest_version": 3,
    "name": "Audible Tab Identifier",
    "version": "0.0.1",
    "description": "An extension that identifies tabs that are making sound and gives you the option to mute/unmute those/the tab(s) of interest.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://*/*"
          ]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}